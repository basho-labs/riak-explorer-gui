{{#dashboard-module class=''}}
  {{#dashboard-module-header}}
    <div class="action-buttons">
      <span class="update action-button" {{action 'updateSchema' model}}>
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Update Schema
      </span>

      {{#link-to 'search-schema' model.cluster.id model.name class='cancel action-button' }}
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        Cancel
      {{/link-to}}
    </div>
  {{/dashboard-module-header}}
  {{#dashboard-module-body}}
    <pre class="editable">
    <code>
      {{content-editable
      value=model.content
      type="html"}}
    </code>
  </pre>
  {{/dashboard-module-body}}
{{/dashboard-module}}
