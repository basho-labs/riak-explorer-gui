{{#dashboard-module}}
  {{#dashboard-module-header}}
    <span>&nbsp;</span>
    <div class="module-actions">
      {{#link-to 'search-schema.edit' model.cluster.id model.name class='edit action-button' }}
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit Schema
      {{/link-to}}

      <a href={{model.url}} target="_blank" class='raw action-button'>
        <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
        View Raw
      </a>
    </div>
  {{/dashboard-module-header}}
  {{#dashboard-module-body}}
    {{#code-highlighter language-type='XML'}}
      {{model.content}}
    {{/code-highlighter}}
  {{/dashboard-module-body}}
{{/dashboard-module}}
