{{#dashboard-module class='medium-width'}}
  {{#dashboard-module-header label='Object Contents'}}
    <div class="module-actions">
      <a href="{{model.url}}" class="action-button raw">
        <span class="glyphicon glyphicon-download" aria-hidden="true"></span>
        View raw
      </a>

      {{!--{{#link-to 'riak-object.edit' model classNames='action-button edit' }}--}}
        {{!--<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>--}}
        {{!--Edit Object--}}
      {{!--{{/link-to}}--}}

      {{!--<button type="button" class="action-button delete"--}}
        {{!--{{action 'deleteObject' model}}>--}}
        {{!--<span class="glyphicon glyphicon-trash" aria-hidden="true"></span>--}}
        {{!--Delete--}}
      {{!--</button>--}}
    </div>
  {{/dashboard-module-header}}
  {{#dashboard-module-body}}
    <div class="object-contents">
      <table class="key-value-table">
        <tbody>
        <tr>
          <td class="key">Content-Type</td>
          <td class="value">{{model.contentType}}</td>
        </tr>
        {{#if model.showContents}}
          <tr>
            <td class="key">Content</td>
            <td class="value">
              <code>{{model.contents}}</code>
            </td>
          </tr>
        {{/if}}
        </tbody>
      </table>
    </div>
  {{/dashboard-module-body}}
{{/dashboard-module}}

{{#dashboard-module class='medium-width'}}
  {{dashboard-module-header label='Object Metadata'}}

  {{#dashboard-module-body}}
    {{#ivy-tabs}}
        {{#ivy-tab-list}}
          {{#ivy-tab}}General{{/ivy-tab}}
          {{#ivy-tab}}Secondary Indexes{{/ivy-tab}}
          {{#ivy-tab}}Custom Headers{{/ivy-tab}}
        {{/ivy-tab-list}}

        {{#ivy-tab-panel}}
          {{table.object-version object=model}}
        {{/ivy-tab-panel}}

        {{#ivy-tab-panel}}
          {{table.object-headers headers=model.indexes}}
        {{/ivy-tab-panel}}

        {{#ivy-tab-panel}}
          {{table.object-headers headers=model.headersCustom}}
        {{/ivy-tab-panel}}
      {{/ivy-tabs}}
  {{/dashboard-module-body}}
{{/dashboard-module}}


