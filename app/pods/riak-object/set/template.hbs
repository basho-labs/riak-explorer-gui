{{#dashboard-module}}
  {{dashboard-module-header label='Object Details'}}
  {{#dashboard-module-body}}
    {{#if model.isLoaded}}
      {{object-contents-set model=model deleteObject="deleteObject"
      removeElement="removeElement" addElement="addElement"}}
    {{else}}
      {{loading-spinner}}
    {{/if}}
  {{/dashboard-module-body}}
{{/dashboard-module}}
