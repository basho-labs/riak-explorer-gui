<div class="row">
    <div class="col-sm-12">
        <div class="row property-header">
            <div class="col-sm-10">
                <strong>{{title}} Properties</strong>
            </div>
            <div class="col-sm-2">
                {{#if model.isActive}}
                <span class="glyphicon glyphicon-ok-sign" aria-hidden="true"></span>
                <span class="status-ok">Activated</span>
                {{else}}
                <span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span>
                <span class="status-disabled">Inactive</span>
                {{/if}}
            </div>
        </div>
        <div class="row">
            {{#em-tabs configName="default"}}
                {{#em-tab-list}}
                    {{#em-tab}}Overview{{/em-tab}}
                    {{#em-tab}}Advanced Properties{{/em-tab}}
                {{/em-tab-list}}
                {{#em-tab-panel}}
                    {{bucket-properties-overview model=model props=model.props}}
                {{/em-tab-panel}}
                {{#em-tab-panel}}
                    {{bucket-properties-list model=model}}
                {{/em-tab-panel}}
            {{/em-tabs}}
        </div>

    </div>
</div>
