<table class="table">
<thead>
    <tr>
        <th>Index</th>
        <th>Schema</th>
        <th>n_val</th>
    </tr>
</thead>
<tbody>
{{#each indexes as |index|}}
    <tr>
        <td>{{link.link-index searchIndex=index}}</td>
        <td><a href={{index.schemaUrl}}>{{index.schema}}</a></td>
        <td>{{index.nVal}}</td>
    </tr>
{{else}}
    No indexes found.
{{/each}}
</tbody>
</table>
