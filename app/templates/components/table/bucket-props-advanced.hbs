{{#each model.propsWithHelp as |prop|}}
  <tr>
    <td class='key'>
      {{prop.name}}
      {{tooltip.bucket-props
      itemKey=prop.key
      itemDescription=prop.description
      itemDefaultValue=prop.default
      itemIsEditable=prop.editable
      itemSchemaType=prop.json_schema_type}}
    </td>
    <td class='value'>
      <code>
        {{#if (is-array prop.value)}}
          [{{prop.value}}]
        {{else}}
          {{prop.value}}
        {{/if}}
      </code>
    </td>
  </tr>
{{/each}}